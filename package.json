{
  "name": "v-verify-monorepo",
  "private": true,
  "version": "1.0.0",
  "workspaces": [
    "apps/*"
  ],
  "scripts": {
    "dev:web": "npm run dev -w apps/web",
    "build:web": "npm run build -w apps/web",
    "preview:web": "npm run preview -w apps/web",
    "dev:server": "npm run dev -w apps/server",
    "start:server": "npm run start -w apps/server",
    "build:server": "npm run build -w apps/server",
    "build": "npm run build:web && npm run build:server",
    "dev": "npm run dev:web & npm run dev:server"
  }
}
